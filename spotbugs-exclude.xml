<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="EI_EXPOSE_REP,EI_EXPOSE_REP2,VA_FORMAT_STRING_USES_NEWLINE"/>
    </Match>
    <Match>
        <Class name="~.*\.Immutable.*"/>
    </Match>
    <Match>
        <Class name="~.*\.Modifiable.*"/>
    </Match>
    <Match>
        <Source name="~.*\.groovy"/>
    </Match>
</FindBugsFilter>
